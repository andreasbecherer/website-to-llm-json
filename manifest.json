{
  "manifest_version": 3,
  "name": "The Skeletonizer",
  "version": "4.0",
  "description": "Extracts website layout & geometry for AI Vibe Coding.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}